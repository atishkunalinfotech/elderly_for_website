<div class="faq">
  <div class="container">
    <div class="row" style="margin-top:150px;">
      <div class="span12">
       <p>
          <%= link_to 'Add Video', new_elder_video_path %>
       </p>
        <h1>Video Gallary</h1>

        <table width="100%" border="0" class="table table-bordered table-striped">
          <thead>
            <tr>
              <th><%= "Video Name" %></th>
              <th><%= "Video Image" %></th>
            </tr>
          </thead>
          <tbody>
            <% @elder_videos.each do |elder_video| %>
              <tr>
                <td><%= elder_video.name %></td>
                <td><%= image_tag(elder_video.video.url(:thumb)) %></td>
                <td><%= link_to 'Show', elder_video %></td>
                <td><%= link_to 'Edit', edit_elder_video_path(elder_video) %></td>
                <td><%= link_to 'Destroy', elder_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                <td><%= link_to 'share',update_share_video_path(:elder_video => elder_video),method: 'post' %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
        <br />
      </div>
    </div>
  </div>
</div>