<div class="faq">
  <div class="container">
    <div class="row" style="margin-top:150px;">
      <div class="span12">
       <!-- <p>
          <%#= link_to 'New Student', new_admin_student_path %>
       </p> -->
        <h1>All Users</h1>

        <table width="100%" border="0" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th><%= "Full Name" %></th>
      <th><%= "Profile Pic" %></th>
      <th><%= "gender" %></th>
      <th><%= "Age" %></th>
      <th><%= "country" %></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.fullname rescue nil %></td>
        <td><%= image_tag user.photo.url(:thumb) rescue nil %></td>
        <td><%= user.gender rescue nil %></td>
        <td><%= user.age rescue nil %></td>
        <td><%= ISO3166::Country[user.country] rescue nil %></td>
        <td><%= link_to 'Destroy', user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
        <br />
      </div>
    </div>
  </div>
</div>
